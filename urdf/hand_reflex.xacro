      <inertial>
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <mass value="0.1"/>
          <inertia
            ixx="0.0" ixy="0.0" ixz="0.0"
            iyy="0.0" iyz="0.0"
            izz="0.0"/>
      </inertial>
    </link>
    <gazebo reference="rail">
      <selfCollide>true</selfCollide>
      <material>Gazebo/White</material>
    </gazebo>

    <link name="base_link">
      <visual>
